{
	"name": "Node.js",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VARIANT": "14" }
	},
	"settings": { 
		"terminal.integrated.shell.linux": "/bin/zsh",
		"editor.formatOnSave": true,
		"editor.tabSize": 2,
		"editor.insertSpaces": true,
		"editor.detectIndentation": false
	},
	"extensions": [
		"dbaeumer.vscode-eslint"
	],
	"remoteUser": "node"
}